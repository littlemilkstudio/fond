{
  "parser": "babel-eslint",  
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
        "jsx": true
    }
  },
  "env": {
    "es6":     true,
    "browser": true,
    "node":    true,
    "commonjs": true 
  },   
  "extends": [
    "eslint:recommended", 
  ],
  "rules": {
    "strict": 0,
    "quotes": ["error", "single"],
    "semi": [1, "always"],
    "no-console": 1
  }
}